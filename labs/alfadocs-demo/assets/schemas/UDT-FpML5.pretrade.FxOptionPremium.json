  "Source" : {
    "@type" : "alfa.rt.model.Union__ModelBaseNode_Source",
    "Contents" : "/#\n   A type that specifies the premium exchanged for a single option trade or option strategy.\n #/\nrecord FpML5.pretrade.FxOptionPremium {\n  # A reference to the party responsible for making the payments defined by this structure.\n  payerPartyReference : FpML5.pretrade.PartyReference\n\n  # A reference to the account responsible for making the payments defined by this structure.\n  payerAccountReference : FpML5.pretrade.AccountReference?\n\n  # A reference to the party that receives the payments corresponding to this structure.\n  receiverPartyReference : FpML5.pretrade.PartyReference\n\n  # A reference to the account that receives the payments corresponding to this structure.\n  receiverAccountReference : FpML5.pretrade.AccountReference?\n\n  # The payment date, which can be expressed as either an adjustable or relative date.\n  paymentDate : FpML5.pretrade.AdjustableOrRelativeDate\n\n  # Non negative payment amount.\n  paymentAmount : FpML5.pretrade.NonNegativeMoney?\n\n  # The information required to settle a currency payment that results from a trade.\n  settlementInformation : FpML5.pretrade.SettlementInformation?\n\n  # This is the option premium as quoted. It is expected to be consistent with the premiumAmount and is for information only.\n  quote : FpML5.pretrade.PremiumQuote?\n\n  id : string?\n\n}"
  },
