  "Source" : {
    "@type" : "alfa.rt.model.Union__ModelBaseNode_Source",
    "Contents" : "/#\n   Describes a schedule of fixing dates as a parametric description, an explicit list of dates or both.\n #/\nrecord FpML5.confirmation.FxWeightedFixingSchedule {\n  # Date adjustments applied to the schedule including the business day convention and the business centers.\n  dateAdjustments : FpML5.confirmation.BusinessDayAdjustments?\n\n  # An explicit list of dates in the schedule. Where a conflict every existed between parametric and explicit dates, the explicit list of dates would takes precedence.\n  fixing : FpML5.confirmation.FxFixingObservation\n\n  # Start of the schedule.\n  startDate : date\n\n  # End of the schedule.\n  endDate : date\n\n  # Defines the frequency at which calculation period end dates occur within the period schedule and their roll date convention.\n  observationFrequency : FpML5.confirmation.ObservationFrequency\n\n  # A day type classification, e.g. Business, Calendar, etc.\n  dayType : string\n\n  # A pointer style reference to a set of financial business centers defined elsewhere in the document. This set of business centers is used to determine whether a particular day is a business day or not.\n  businessCentersReference : FpML5.confirmation.BusinessCentersReference\n\n  businessCenters : FpML5.confirmation.BusinessCenters\n\n  # The number of fixing points in the fixing schedule.\n  numberOfFixings : int( 0, * )\n\n}"
  },
